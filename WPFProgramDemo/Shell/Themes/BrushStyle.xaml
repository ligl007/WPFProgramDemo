<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--文本控件颜色-->
    <SolidColorBrush x:Key="OuterBlueBrush" Color="#FF8AF0FF"/>

    <!--TextBox  画刷-->
    <SolidColorBrush x:Key="TextBoxBorder" Color="#FFADB2B6"/>

    <!--字体不可用颜色-->
    <SolidColorBrush x:Key="EnableForeground" Color="#FF939393"/>

    <!--默认背景色-->
    <SolidColorBrush  x:Key="TextBoxBackground"  Color="White"/>
    
    <!--只读时的边框色-->
    <SolidColorBrush x:Key="TextBoxBorderRead" Color="#7FADB2B6"/>
     
    <!--只读时的背景色-->
    <SolidColorBrush x:Key="TextBoxBgRead" Color="#7FFFFFFF"/>

    <!--三个像素白色-->
    <LinearGradientBrush x:Key="TextBoxTopBrush" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFD1D1D1" Offset="0"/>
        <GradientStop Color="#FFF4F4F4" Offset="1"/>
    </LinearGradientBrush>

    <!--button  brush-->
    <!--按钮正常颜色-->
    <LinearGradientBrush x:Key="normalBackBrush" StartPoint="0.499999,0.0364487" EndPoint="0.499999,0.963558">
        <GradientStop Color="#FFFFFFFF" Offset="0"/>
        <GradientStop Color="#FFE2E7E8" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="normalBorderBrush"	Color="#FFABABAB"/>

    <!--按钮滑过颜色-->
    <LinearGradientBrush x:Key="mouseBackBrush" StartPoint="0.499999,0.0364487" EndPoint="0.499999,0.963558">
        <GradientStop Color="#FFD9EEF8" Offset="0"/>
        <GradientStop Color="#FFBBDFF1" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="mouseBorderBrush"	Color="#FF458CB3"/>

    <!--按钮按下颜色-->
    <LinearGradientBrush x:Key="pressBackBrush" StartPoint="0.5,0.0364487" EndPoint="0.5,0.963558">
        <GradientStop Color="#FFD2D9DD" Offset="0"/>
        <GradientStop Color="#FFE8ECEE" Offset="1"/>
    </LinearGradientBrush>

    <!--按钮不可用的边框颜色-->
    <SolidColorBrush x:Key="EnableBrush" Color="#FFC5C5C5"/>

    <!--按钮不可用的背景颜色-->
    <LinearGradientBrush x:Key="EnableBackBrush" StartPoint="0.5,0.0364488" EndPoint="0.5,0.963558">
        <GradientStop Color="#FFF6F6F6" Offset="0"/>
        <GradientStop Color="#FFEBEBEB" Offset="1"/>
    </LinearGradientBrush>

    <!--按钮一个像素的内框颜色-->
    <SolidColorBrush x:Key="wborderbrush" Color="#B2FFFFFF"/>

    <!--聊天记录白色画刷-->
    <LinearGradientBrush x:Key="RecordBackBrush" EndPoint="0.5,1" 
                             MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">

        <!--<GradientStop Color="#FFFDFDFD" Offset="1"/>
        <GradientStop Color="#FFF3F3F3"/>-->
        <GradientStop Color="#FFFDFDFD" Offset="0"/>
        <GradientStop Color="#FFF3F3F3" Offset="1"/>
    </LinearGradientBrush>

    <!--Widnow 头萌版颜色-->
    <LinearGradientBrush x:Key="WindowTitleBackground" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#7FFFFFFF" Offset="0"/>
        <GradientStop Color="#0CFFFFFF" Offset="1"/>
    </LinearGradientBrush>
    <!--聊天Toolbar画刷-->
    <SolidColorBrush x:Key="ChatToolBorderBrush" Color="#4C9AD0F5"/>
    <!--聊天字体Toolbar画刷-->
    <SolidColorBrush x:Key="ChatToolBarBrush"  Color="Red"/>

    <!--查找群三个按钮 鼠标滑过画刷-->
    <SolidColorBrush x:Key="FindButtonMoveBrush" Color="#FF627CCE"></SolidColorBrush>
    <!--查找群三个按钮 鼠标选中以及按下画刷-->
    <SolidColorBrush x:Key="FindButtonCheckBrush" Color="#FF88CE62"></SolidColorBrush>
    
    <!--鼠标滑过背景色-->
    <SolidColorBrush x:Key="MouseOverBrush" Color="#FFFFF9D2"/>
    
    <!--蓝色字体颜色值-->
    <SolidColorBrush x:Key="ForegroundBrush"  Color="#FF0066cc"/>

    <!--9ad0f5 30%透明 5-1-->
    <SolidColorBrush x:Key="Brush51" Color="#4C9AD0F5"/>

    <!--ffffff 10%透明-->
    <SolidColorBrush x:Key="Brush21" Color="#19FFFFFF"/>
    
    <!--ffffff 20%透明-->
    <SolidColorBrush x:Key="Brush20" Color="#33FFFFFF"/>

    <!--ffffff 30%透明 2-2-->
    <SolidColorBrush x:Key="Brush22"  Color="#4CFFFFFF"/>

    <!--ffffff 90%透明 4-1-->
    <SolidColorBrush x:Key="Brush41"  Color="#E5FFFFFF"/>

    <!--ffffff 60%透明 6-1-->
    <SolidColorBrush x:Key="Brush61"  Color="#99FFFFFF"/>

    <!--ffffff 50%透明-->
    <SolidColorBrush x:Key="Brush50" Color="#7FFFFFFF"/>
